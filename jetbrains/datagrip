#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2024-12-16T15:14:24.828937

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/taylorknapp/Library/Application Support/JetBrains/Toolbox/apps/datagrip/ch-0/233.11799.296/DataGrip.app/Contents/MacOS/datagrip" $wait --args "${ideargs[@]}"