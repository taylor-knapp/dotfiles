#!/bin/bash
#Generated by JetBrains Toolbox 1.21.9712 at 2025-08-27T21:05:14.319558

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/taylorknapp/Library/Application Support/JetBrains/Toolbox/apps/IDEA-C/ch-0/252.23892.409/IntelliJ IDEA CE.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"